<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
	<RelativeLayout android:id="@+id/rllyAutoIp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:minHeight="50dip" >
		
		<Switch android:id="@+id/swAutoIp"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentRight="true"
		    android:layout_centerVertical="true"
		    android:thumb="@drawable/switch_inner_holo_light"
		    android:track="@drawable/switch_track_holo_light"
		    android:textOn="@string/auto_ip_on"
		    android:textOff="@string/auto_ip_off"
		    android:textColor="@android:color/primary_text_light" />
		
		<TextView
        	android:id="@+id/tvNoticeWireStatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_toLeftOf="@id/swAutoIp"
		    android:layout_centerVertical="true"
	        android:layout_marginRight="30dip"
            android:textSize="@dimen/text_size_normal"
        	android:textColor="@color/text_color_normal"
            android:text="@string/auto_ip" />
    </RelativeLayout>
    
	<RelativeLayout android:id="@+id/rllyIpAddress"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/rllyAutoIp"
        android:layout_marginTop="20dip"
	    android:gravity="center_vertical"
	    android:minHeight="50dip" >
	
	    <TextView
	        android:layout_width="200dip"
	        android:layout_height="35dip"
	        android:text="@string/ip_address"
	        android:textSize="@dimen/text_size_normal"
	        android:textColor="@color/text_color_normal" />
	
	    <cn.ktc.android.oobe.widget.InputIPAddress
	        android:id="@+id/edtIpFour"
	        android:layout_width="60dip"
	        android:layout_height="35dip"
	        android:layout_alignParentRight="true"
	        android:background="@drawable/edit_text"
	        android:gravity="center"
	        android:inputType="numberDecimal"
	        android:maxLength="3"
	        android:textColor="@color/ip_edit_text"
	        android:cursorVisible="false"
	        android:imeOptions="actionDone|flagNoExtractUi|flagNoFullscreen" />
	
	    <cn.ktc.android.oobe.widget.InputIPAddress
