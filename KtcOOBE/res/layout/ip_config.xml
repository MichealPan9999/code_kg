	        android:textColor="@color/ip_edit_text"
	        android:cursorVisible="false"
	        android:imeOptions="actionDone|flagNoExtractUi|flagNoFullscreen" />
	
	    <cn.ktc.android.oobe.widget.InputIPAddress
	        android:id="@+id/edtIpTwo"
